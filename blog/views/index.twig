{% extends "layout.twig" %}
{% block content %}
    {% for blogPost in blogPosts %}
        <div class="blog-post">
            <h2>{{ blogPost.title }}</h2>
            Fecha 00-00-00 by <a href="">alex</a>
            <div class="blog-post-image">
                <img src="img/Desert.jpg">
            </div>
            <div class="blog-post-content">
                {{ blogPost.content }}
            </div>
        </div>

    {% endfor %}
{% endblock %}
